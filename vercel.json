{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(:path*)",
      "destination": "/public/:path*"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ]
}
